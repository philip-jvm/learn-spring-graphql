scalar LocalDateTime

type Query {
    schedule(coach: Coach!): [FitnessClass!]!
}

enum Coach {
    PHILIP
    PETER
}

type FitnessClass {
    id: ID!
    coach: Coach!
    startsAt: LocalDateTime!
    endsAt: LocalDateTime!
    difficulty: Difficulty!
}

enum Difficulty {
    BEGINNER
    ADVANCED
}
